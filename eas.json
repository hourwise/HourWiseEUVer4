{
  "cli": {
    "version": ">= 7.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "_base_internal": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      }
    },
    "development": {
      "extends": "_base_internal",
      "developmentClient": true,
      "channel": "development",
      "env": {
        "APP_ENV": "development"
      }
    },
    "simulator": {
      "extends": "_base_internal",
      "channel": "staging",
      "ios": {
        "simulator": true
      },
      "env": {
        "APP_ENV": "staging"
      }
    },
    "production": {
      "channel": "production",
      "autoIncrement": true,
      "distribution": "store",
      "android": {
        "buildType": "app-bundle"
      },
      "env": {
        "APP_ENV": "production"
      }
    }
  },
  "submit": {
    "production": {
      "android": {
        "track": "internal",
        "releaseStatus": "draft"
      },
      "ios": {
        "ascAppId": "1234567890",
        "appleTeamId": "TEAMID123"
      }
    }
  }
}
